    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "aws-sdk": "^2.1490.0",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "objection": "^3.0.1",
    "joi": "^17.11.0",
    "joi-password-complexity": "^5.1.0",
    "ethers": "^6.8.1",
    "stripe": "^14.7.0",
    "paypal-rest-sdk": "^1.8.1",
    "cloudinary": "^1.41.0",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "axios": "^1.6.0",
    "express-slow-down": "^2.0.1",
    "sanitize-html": "^2.11.0",
    "xss-clean": "^0.1.4",
    "hpp": "^0.2.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
