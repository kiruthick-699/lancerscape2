    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@types/node-cron": "^3.0.11",
    "@types/sanitize-html": "^2.9.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "eslint": "^8.52.0",
